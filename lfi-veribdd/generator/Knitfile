return b{
    $ lfi.bdd: genbdd
        ./genbdd lfi.bdd.txt
        go run bdd.go lfi.bdd.txt > $output
    $ genbdd: genbdd.o ../../lfi-verify/target/release/liblfiverify.a
        c++ $input -o $output -lbdd -llfiverify
    $ %.o: %.cc
        c++ -c -O2 $input -o $output -I../../liblfi
    $ %.o: %.c
        cc -c -O2 $input -o $output
    include("../../lfi-verify/Knitfile")
}
